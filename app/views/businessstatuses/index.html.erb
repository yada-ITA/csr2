<h1>Listing businessstatuses</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @businessstatuses.each do |businessstatus| %>
      <tr>
        <td><%= businessstatus.name %></td>
        <td><%= link_to 'Show', businessstatus %></td>
        <td><%= link_to 'Edit', edit_businessstatus_path(businessstatus) %></td>
        <td><%= link_to 'Destroy', businessstatus, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Businessstatus', new_businessstatus_path %>
