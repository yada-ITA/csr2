<h1>Listing enginestatuses</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @enginestatuses.each do |enginestatus| %>
      <tr>
        <td><%= enginestatus.name %></td>
        <td><%= link_to 'Show', enginestatus %></td>
        <td><%= link_to 'Edit', edit_enginestatus_path(enginestatus) %></td>
        <td><%= link_to 'Destroy', enginestatus, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enginestatus', new_enginestatus_path %>
