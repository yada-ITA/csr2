<h1>Listing enginemodels</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Modelcode</th>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @enginemodels.each do |enginemodel| %>
      <tr>
        <td><%= enginemodel.modelcode %></td>
        <td><%= enginemodel.name %></td>
        <td><%= link_to 'Show', enginemodel %></td>
        <td><%= link_to 'Edit', edit_enginemodel_path(enginemodel) %></td>
        <td><%= link_to 'Destroy', enginemodel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

